;************************************************************************
;*                                                                      *
;*  T1Scans.Asc has scan code for the RAPDA detector.                   *
;*                                                                      *
;************************************************************************

        INCLUDE 'OMACP.INC'
        INCLUDE 'RAPDA.INC'
        INCLUDE 'HOOKS.INC'

;************************************************************************
;*                                                                      *
;* Routine: INIT                                                        *
;* Purpose: To initialise the FC processor and the detector,            *
;*          and to synchronise with the DMA and DAC processors.         *
;*                                                                      *
;********  Frame Control program  ***************************************

FCINIT  FC_BLOCK

        WAIT
        LDCFG   DCADDR | RAPDA | XMIT15 | REC16 | RECTRIG | RECTEMP
        LDTRKL  NUMSERIAL                       ! TRACKLEN
        LDPCNT  DB_PREC                         ! PCNT

        WAIT
        LOAD    DET_CNTL_BD | NO_LOOPBACK

        WAIT
        LOAD    AD_BD | CALRUN			; activate A/D


        WAIT
        LOAD    SFGHI                           ;Set fixed gain high.

        LDCNTRA 17

FCINT0  WAIT
        LOAD CLR_FIFO                           ;Empty the A/D FIFO.
        JUNTILA FCINT0

        LDCNTRA 40

FCINT1  WAIT                                    ;Send NOPs to kill time.
        LOAD    NO_OP
        JUNTILA FCINT1

        WAIT                                    ;Sync FC $ DC Ends.
        LOAD    NO_OP | CUE                     ;Alert the DC it's time...
        END

;********  DMA Control program  ****************************************

DCSYNC  DMA_BLOCK

        WAIT                                    ;Sync FC $ DC Ends.
        LOAD   NOTHING
        END

;*
;******************************************************************
;*

FCIDNT  FC_BLOCK

        WAIT
        LOAD    SHUTTER_BD | INT_SHUTTER  | CUE | RPP_RTP

        WAIT
        LOAD    DET_CNTL_BD | NO_LOOPBACK | CUE | IPP

        LDCNTRA 200 

FCIDNT1 WAIT                                    ;Send NOPs to kill time.
        LOAD    RAFG | $0000                    ;goto pixel 0.

        WAIT
        LOAD    NO_OP
        JUNTILA FCIDNT1

        WAIT                                    ;Sync FC $ DC Ends.
        LOAD    NO_OP | CUE
        END

;********  DMA Control program  ****************************************

DCIDNT  DMA_BLOCK

        WAIT
        LOAD   REPLACE | LD_ADDR

        WAIT                                    ;Sync FC $ DC Ends.
        LOAD   NOTHING
        END


;************************************************************************
;*                                                                      *
;* Routine: IDX                                                         *
;* Purpose: To identify a single channel controller                     *
;*                                                                      *
;************************************************************************

;*
;******************************************************************
;*
 FCIDX   FC_BLOCK
 
        WAIT
        LOAD    NO_OP
        LDCFG   DCADDR | QNS | XMIT13 | REC18 | RECTRIG | RECTEMP
        WAIT
        LOAD    NO_OP
        LDTRKL  NUMSERIAL
        WAIT
        LOAD    NO_OP
        LDPCNT  DB_PREC

        WAIT                                    ;Aux Bus Addrs. for TSC:
        LOAD    $1100 | MS10                    ;1100 is A/D Board

        WAIT
        LOAD    $1400 | A_CHANNEL | RS_XYZ      ;1400 is Clock/Bias

        WAIT
        LOAD    $1600 | INT_SHUTTER             ;1600 is Shutter/Pulser

        WAIT
        LOAD    $1000 | ACHANNEL                ;1000 is Analog Processor
                                                
        WAIT
        LOAD    $1F00 | NO_LOOPBACK             ;1F00 is Detector Controller

        LDCNTRA 100

FCIDX1  WAIT                                    ;Send NOPs to kill time.
        LOAD    NO_OP
        JUNTILA FCIDX1

        WAIT
        LOAD    $1600 | INT_SHUTTER  | CUE | RPP_RTP

        WAIT
        LOAD    $1F00 | NO_LOOPBACK | CUE | IPP

        LDCNTRA 100

FCIDX2  WAIT                                    ;Send NOPs to kill time.
        LOAD    NO_OP
        JUNTILA FCIDX2

        WAIT                                    ;Sync FC $ DC Ends.
        LOAD    NO_OP | CUE
        END
;********  DMA Control program  ****************************************

DCIDX  DMA_BLOCK

        WAIT
        LOAD   REPLACE | LD_ADDR                ;Store detector ID

        WAIT
        LOAD   REPLACE                          ;Store Shutter ID

        WAIT                                    ;Sync FC $ DC Ends.
        LOAD   NOTHING
        END

;******************************************************************

;*
;******************************************************************
;  The following is a way to get the addresses of the various variables
;  to load, ... later ; this code never gets executed.
;*

FCCONF  FC_BLOCK
        LDCNTRA  NUMIMAGE         ! IACTIVE      ;ActiveY
        LDCNTRA  NUMSERIAL        ! SACTIVE      ;ActiveX
        LDCNTRA  DEAD_IMAGE_LEAD  ! DUMYTR
        LDCNTRA  DEAD_IMAGE_LAST  ! DUMYTR
        LDCNTRA  0                ! DUMXLD
        LDCNTRA  0                ! DUMXTR
        LDCNTRA  $FD              ! DET_ID

        END

;************************************************************************
;*                                                                      *
;* Routine: KC (Keep Clean)                                             *
;* Purpose: To maintain the detector in a low dark current state.       *
;*                                                                      *
;********  Frame Control program  ***************************************

FCKC    FC_BLOCK

        WAIT
        LOAD NO_OP | SET_B | RS_A ; use B trig to diagnose keepclean
        WAIT
        LOAD TE_SERVO_BD | ((10-(-40))&$FE) | RPP_RTP | CUE ! TEMP_TAG

        WAIT                                    ;Send NOP to kill time.
        LOAD    NO_OP

        GOSUB   CLEANSR           ; Do a keep clean.

        LDCNTRA 60 

FCKC1   WAIT                                    ;Send NOPs to kill time.
        LOAD CLR_FIFO                           ;Empty the A/D FIFO.
 ;       LOAD    NO_OP
        JUNTILA FCKC1

        WAIT
        LOAD    NO_OP | CUE | RS_B        ; turn off trigger B.
        END

;********  DMA Control program  *********************************

DCKC    DMA_BLOCK

        WAIT
        LOAD REPLACE | LD_ADDR            ;Store TE status

        WAIT
        LOAD    NOTHING                   ;Sync FC Shift Line
        END

;****************************************************************
;        Subroutine to do a keep clean for the RAPDA
;****************************************************************

CLEANSR FC_BLOCK                  ! SER_CLEAN

        WAIT
        LOAD    NO_OP
        LDCNTRB TOTAL_SERIAL      ! S_TOTAL
        WAIT
        LOAD    CLR_FIFO                         ;Empty the A/D FIFO.
        WAIT
        LOAD    RAFG | $0000                    ;goto pixel 0.
        JUNTILB CLEAN1

CLEAN1  WAIT                                    ;Trash all data points.
        LOAD    ANWFG | IPP | SET_B 
        JUNTILB CLEAN1

        WAIT                                     ;Goto pxl 0, park.
        LOAD    RAFG | $0000

        RETURN

; ************************** Real RAPDA ********************************

; All the RAPDA scan programs have common endings, the FC have one ending,
; and the DC have a similar ending.  Therefore, only one ending is really
; needed for the FC programs, and another for the DC programs; all RAPDA
; FC scan programs load counter A and do RAFG to the first pixel, then
; do JUNTILA FC_RDLP (read loop).  Similarly, all DC programs load counter B
; and do JUNTILB DC_RDLP.  Note this scheme depends on never loading the
; counter with 1, so the jump is always taken.  In RAPDA, the minimum 
; number of pixels is 8, so this seems safe;  however, the case could
; arise in grouping where 8 pixels are scanned, grouped by 8, so only 1
; point would be placed in memory by the DC program.  This brings up the
; second and, from the FC and DC program standpoint, simpler way to unify
; the programs.  That is to have the first FC (DC) program use GOSUB:
;
;          GOSUB FC_COMN                      ; Sets up the RETURN address.
;  FC_COMN JUNTILA FC_RDLP                    ; The common ending for FC's.
;          JUNTILA FC_END                     ; If A was 1, do not loop
;  FC_RDLP WAIT	            
;          LOAD    ANWFG | IPP | SET_B | CUE   
;          JUNTILA FC_RDLP
;  FC_END  WAIT
;          LOAD    RAFG | $000  | CUE  ; Goto pixel 0. Sync FC End
;          END
;
;          GOSUB DC_COMN                      ; Sets up the RETURN address.
;  DC_COMN JUNTILB DC_RDLP                    ; The common ending for DC's.
;          JUNTILB DC_END                     ; If B was 1, do not loop
;  DC_RDLP WAIT	            
;          LOAD    AD_P_MEM       ! DC_ACTION
;          JUNTILB DC_RDLP
;  DC_END  WAIT
;          LOAD    NOTHING
;          END
;
; Then all subsequent FC (DC) programs could load counter A, RAFG to the
; first pixel, and terminate with the RETURN statement.  This saves two
; bytes per FC (DC) program.  In the event B was 1, there might not be enough
; time to do this??? a RETURN, JUNTILB, JUNTILB, WAIT, LOAD NOTHING
; (6 + 12 + 12 + 3 + 6 = 39 cycles or about 2 æsec).
;
; The reason this is not being done first is it makes the first FC and
; DC program longer than the rest, and stuffing TableI and Table II becomes
; more involved.  Once I am comfortable with how to do that, I will
; change over.
;

FC_RDLP FC_BLOCK
        WAIT	            
        LOAD    ANWFG | IPP | SET_B | CUE   
        JUNTILA FC_RDLP

        WAIT
        LOAD    RAFG | $000 | CUE            ;Goto pixel 0
;       WAIT
;       LOAD    RS_A  | CUE             ; Sync FC End
 
        END

DC_RDLP DMA_BLOCK
        WAIT	            
        LOAD    AD_P_MEM              ! DC_ACTION
        JUNTILB DC_RDLP

        WAIT                                     ;Sync. FC End
        LOAD    NOTHING
        END


; The Delay Read cycle.  If both IN_COUNT and OUT_COUNT are zero,
; then use the address FC_RDA0 for the Table 1, otherwise use FC_D0A.
; Always use FC_RDA0 for Table 2.  Note that the minimum number of
; pixels allowed (RD0_CNT) is 8.  The FBase address was pulled
; from Table 1 or 2.

; The FC block for delay read cycle 0A; This is the first delay read.
; The placement is crucial, since the driver assumes 16 delay reads,
; all of the same length, to find the addresses to set the delay loops,
; first pixels, number of pixels..etc..

FC_D0A  FC_BLOCK                      ! DELAY_READ_0
        WAIT
        LOAD    NO_OP                               ; a 3.2 æsec delay.
        LDCNTRA 0                     ! OUTER_COUNT ; the outer counter delay
        LDCNTRB 0                     ! INNER_COUNT ; the inner counter delay
FC_0ALP WAIT
        LOAD    NO_OP                               ; a 3.2 æsec delay.
        JUNTILB FC_0ALP
        JUNTILA FC_0ALP

FC_DRDA WAIT                          ! READ_OFFSET ; start of read loop.
        LOAD    NO_OP | SET_A | SET_B               ; use A trig.
        LDCNTRA 8                     ! READ_COUNT ; number to read
        WAIT
        LOAD    RAFG | RPP_RTP | CUE  ! READ_START ; Goto start pixel.
        JUNTILA FC_RDLP


; The FC block for delay read cycle 0B; this must be the second one.
FC_D0B  FC_BLOCK                      ! DELAY_READ_1
        WAIT
        LOAD    NO_OP
        LDCNTRA 0
        LDCNTRB 0
FC_0BLP WAIT
        LOAD    NO_OP                               ; a 3.2 æsec delay.
        JUNTILB FC_0BLP
        JUNTILA FC_0BLP

FC_DRDB WAIT
