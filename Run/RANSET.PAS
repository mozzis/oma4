PROGRAM RANSET;

VAR I,J,SKIP:INTEGER;

BEGIN
  BUFLEN:=10;
  SKIP:=10;
  DS_POINTS(10);
  DS_POINTMODE(1);
  DS_POINTS(220);
  FOR I:=0 TO 9 DO BEGIN
    FOR J:=0 TO 19 DO BEGIN
      WRITELN('POINT',I*20+J+1,'X0',1*20+J+1+SKIP,'DELTA',1);
      DS_POINT(I*20+J+1);
      DS_X0((I*20)+J+1+SKIP);
      DS_DELTAX(1)
    END;
    SKIP:=SKIP+1;
  END;
END.

